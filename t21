int x[4];
int y[4];
int i;
bool z;
x[0] = 1;
x[1] = 2;
x[2] = 3;
x[3] = 4;
y[0] = 5;
y[1] = 3;
y[2] = 3;
y[3] = 2;
for (i = 0; i < 4; i=i+1;) {
    z = x[i] == y[i];
    printBool z;
}
printlines(2);
for (i = 0; i < 4; i=i+1;) {
    z = x[i] <= y[i];
    printBool z;
}
printlines(2);
for (i = 0; i < 4; i=i+1;) {
    z = x[i] >= 3 &&  (y[i] <= 5);
    printBool z;
}
printlines(2);
for (i = 0; i < 4; i=i+1;) {
    z = x[i] > 3 ||  !(y[i] >= 5);
    printBool z;
}
printlines(1);